db.message.insert({
	"receiver" : "sen_zheng",
	"sender" : "mengxiong",
	"content" : "This is a message from mengxiong",
	"messageDate" : ISODate("2015-11-20T00:00:00Z"),
	"read" : false,
	"new" : true
})

